public IToken nextToken() {
    input.skipSpaceTabLineBreak();
    input.markLexemeStart();
    int state = startState;
    int lastAccepted = 0;
    int extraLoaded = 0;
    while (input.available()) {
        byte b = input.next();
        state = goTo[state][b];
        if (state == 0) {
            input.retract();
            break;
        }
        if (accepted[state]) {
            lastAccepted = state;
            extraLoaded = 0;
        } else {
            extraLoaded++;
        }
    }
    String lexeme = input.lexeme();
    input.retract(extraLoaded);
    if (lastAccepted == 0) {
        return null;
    }
    return tokens[lastAccepted].fromLexeme(lexeme);
}
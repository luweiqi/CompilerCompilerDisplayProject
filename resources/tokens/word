private static final Map<String, Integer> keyWords;

static {
    keyWords = new HashMap<>();
    init();
}

private int type;
private String lexeme;

@Override
public int type() {
    return type;
}

@Override
public IToken fromLexeme(String lexeme) {
    this.lexeme = lexeme;
    type = keyWords.getOrDefault(lexeme, TokenTypes.WORD);
    return this;
}

@Override
public String toString() {
    String res = keyWords.containsKey(lexeme) ? "keyWord" : "id";
    return res + ":" + lexeme;
}

public String lexeme() {
    return lexeme;
}